<div class="container">
		<form class="login-form" action="/customer" method="POST">
				<h1 id="login">
					Create a New Proile!
				</h1>
			  	<input type="text" class="login-username" name="name"  required="true" placeholder="Your good name?" />
				<input type="text" class="login-password" name="city" required="true" placeholder="city?" />
				<input type="number" class="login-password" name="age" required="true" placeholder="age" />
			 	<input type="text" class="login-password" name="work" required="true" placeholder="what do you do?" />
			  	<input type="text" class="login-password" name="contact" required="true" placeholder="contact number!" />

			  <input type="submit" name="Login" value="Signup" class="login-submit" />	
			</form>
			<a href="/logout"> <input type="submit" value="Logout" class="login-submit" /></a>
