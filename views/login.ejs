<%- include("partials/header_provider") %>	
	<div class="container">
			<form class="login-form" action="/login" method="POST">
				<h1 id="login">
					Enter your credentials!
				</h1>
			  <input type="text" class="login-username" name="username" autofocus="true" required="true" placeholder="username" />
			  <input type="password" class="login-password" name="password" required="true" placeholder="password" />
			  <input type="submit" name="Login" value="Login" class="login-submit" />
			</form>
			
			<a href="/register" class="login-forgot-pass">Don't have an account? CREATE NOW!</a>
			<div class="underlay-photo"></div>
	<div class="underlay-black"></div> 
</div>
      
